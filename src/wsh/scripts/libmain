function callmain (arg)
	argc = 0
	print(" [*] preparing main(", #arg, ")")

	for i,v in ipairs(arg) do
		argc=argc+1
	end
	argv = balloc(argc+1)
	ac = 0;
	for i,v in ipairs(arg) do
		bset(argv,ac,v);
		ac=ac+1
	end

	print(" [*] Doing call")
	return main(argc, argv, _environ)
end


